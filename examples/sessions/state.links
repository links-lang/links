typename State(a) = AP (!a.End);

sig newCell : (a) ~> State(a)
fun newCell(v) {
  var x = new();
  var _ = spawn {send(v, accept(x))};
  x
}

sig put : (State(a), a) ~> ()
fun put(x, v) {
  var _ = receive(request(x)).1;
  var _ = spawn {send(v, accept(x))};
  ()
}

sig get : (State(a)) ~> a
fun get(x) {
  var v = receive(request(x)).1;
  var _ = spawn {send(v, accept(x))};
  v
}
