(env
 (development
  (flags (:standard -safe-string -dtypes -w Ae-44-45-60 -g -cclib -lunix -thread)))
 (release
  (flags (:standard -safe-string -dtypes -warn-error Ae-44-45-60 -cclib -lunix -thread))))
  
(install
 (package links)
 (section doc)
 (files (INSTALL as README)))

(include install.inc)
