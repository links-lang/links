(jbuild_version 1)

(executable
 ((name links)
  (public_name linx)
  (package links)
  (modes (native))
  (flags (:standard -safe-string -dtypes -w Ae-44-45-60 -g -cclib -lunix -thread))
  (link_flags -linkall)
  (libraries (findlib.dynload threads dynlink links-core))))

(install
 ((section lib)
  (files (prelude.links))
  (package links)))

(rule
 ((targets (prelude.links))
  (action (copy ${SCOPE_ROOT}/prelude.links prelude.links))))