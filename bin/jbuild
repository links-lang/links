(jbuild_version 1)

(executable
 ((name links)
  (public_name links)
  (package links)
  (modes (native))
  (flags (:standard -safe-string -dtypes -w Ae-44-45-60 -g -cclib -lunix -thread))
  (link_flags -linkall)
  (libraries (threads dynlink links-core))))


(install
 ((section bin)
  (files ((links.exe as linx)))
  (package links)))
